pipeline {
    agent any

    stages {
        stage('mvn 3.9.6') {
            steps {
                echo "Version Checked"
            }
        }
        stage('mvn Install') {
            steps {
                echo "mvn installation done"
            }
        }
        stage('package') {
            steps {
                echo "packages installation"
            }
        }
    }
}
